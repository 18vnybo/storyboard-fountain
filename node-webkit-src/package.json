{
  "name": "storyboard-fountain",
  "version": "0.0.1",
  "main": "index.html",
  "dependencies": {
    "aws-sdk": "~2.0.0-rc.14",
    "tmp": "0.0.23",
    "co": "~3.0.6",
    "thunkify": "~2.1.1",
    "co-fs": "~1.2.0",
    "ncp": "~0.5.1",
    "request": "~2.36.0",
    "clone": "~0.1.15",
    "underscore": "~1.6.0",
